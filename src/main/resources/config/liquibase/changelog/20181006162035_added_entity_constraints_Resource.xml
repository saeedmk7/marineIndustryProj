<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Resource.
    -->
    <changeSet id="20181006162035-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="resources_id"
                                 baseTableName="resource_document"
                                 constraintName="fk_resource_document_resources_id"
                                 referencedColumnNames="id"
                                 referencedTableName="resource"/>
        <addForeignKeyConstraint baseColumnNames="documents_id"
                                 baseTableName="resource_document"
                                 constraintName="fk_resource_document_documents_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document"/>
        
    </changeSet>
</databaseChangeLog>
